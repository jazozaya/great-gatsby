{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],

    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
    },

    "rules" : {
      "import/no-webpack-loader-syntax" : "off",
      "import/extensions": "never"
      }

}
